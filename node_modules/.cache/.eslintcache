[{"/Users/julianahyc/Desktop/Senior/SeniorüçÅ/portfolio/src/index.js":"1","/Users/julianahyc/Desktop/Senior/SeniorüçÅ/portfolio/src/App.js":"2","/Users/julianahyc/Desktop/Senior/SeniorüçÅ/portfolio/src/pages/Navbar.js":"3","/Users/julianahyc/Desktop/Senior/SeniorüçÅ/portfolio/src/pages/Iterative.js":"4","/Users/julianahyc/Desktop/Senior/SeniorüçÅ/portfolio/src/pages/Responsive.js":"5","/Users/julianahyc/Desktop/Senior/SeniorüçÅ/portfolio/src/pages/Persona.js":"6","/Users/julianahyc/Desktop/Senior/SeniorüçÅ/portfolio/src/pages/Development.js":"7","/Users/julianahyc/Desktop/Senior/SeniorüçÅ/portfolio/src/pages/About.js":"8","/Users/julianahyc/Desktop/Senior/SeniorüçÅ/portfolio/src/pages/Home.js":"9","/Users/julianahyc/Desktop/Senior/SeniorüçÅ/portfolio/src/pages/Art.js":"10","/Users/julianahyc/Desktop/Senior/SeniorüçÅ/portfolio/src/pages/ProjectItem.js":"11"},{"size":261,"mtime":1674716333414,"results":"12","hashOfConfig":"13"},{"size":1944,"mtime":1674716333413,"results":"14","hashOfConfig":"13"},{"size":478,"mtime":1674716333417,"results":"15","hashOfConfig":"13"},{"size":36149,"mtime":1674716333416,"results":"16","hashOfConfig":"13"},{"size":9714,"mtime":1674716333418,"results":"17","hashOfConfig":"13"},{"size":12533,"mtime":1674716333417,"results":"18","hashOfConfig":"13"},{"size":4436,"mtime":1674716333415,"results":"19","hashOfConfig":"13"},{"size":146,"mtime":1674716333414,"results":"20","hashOfConfig":"13"},{"size":1614,"mtime":1674716333415,"results":"21","hashOfConfig":"13"},{"size":343,"mtime":1674716333415,"results":"22","hashOfConfig":"13"},{"size":1374,"mtime":1674716333418,"results":"23","hashOfConfig":"13"},{"filePath":"24","messages":"25","suppressedMessages":"26","errorCount":0,"fatalErrorCount":0,"warningCount":0,"fixableErrorCount":0,"fixableWarningCount":0},"1nt920c",{"filePath":"27","messages":"28","suppressedMessages":"29","errorCount":0,"fatalErrorCount":0,"warningCount":2,"fixableErrorCount":0,"fixableWarningCount":0,"source":"30"},{"filePath":"31","messages":"32","suppressedMessages":"33","errorCount":0,"fatalErrorCount":0,"warningCount":0,"fixableErrorCount":0,"fixableWarningCount":0},{"filePath":"34","messages":"35","suppressedMessages":"36","errorCount":0,"fatalErrorCount":0,"warningCount":20,"fixableErrorCount":0,"fixableWarningCount":5,"source":"37"},{"filePath":"38","messages":"39","suppressedMessages":"40","errorCount":0,"fatalErrorCount":0,"warningCount":21,"fixableErrorCount":0,"fixableWarningCount":0,"source":"41"},{"filePath":"42","messages":"43","suppressedMessages":"44","errorCount":0,"fatalErrorCount":0,"warningCount":8,"fixableErrorCount":0,"fixableWarningCount":0,"source":"45"},{"filePath":"46","messages":"47","suppressedMessages":"48","errorCount":0,"fatalErrorCount":0,"warningCount":1,"fixableErrorCount":0,"fixableWarningCount":0,"source":"49"},{"filePath":"50","messages":"51","suppressedMessages":"52","errorCount":0,"fatalErrorCount":0,"warningCount":0,"fixableErrorCount":0,"fixableWarningCount":0},{"filePath":"53","messages":"54","suppressedMessages":"55","errorCount":0,"fatalErrorCount":0,"warningCount":0,"fixableErrorCount":0,"fixableWarningCount":0},{"filePath":"56","messages":"57","suppressedMessages":"58","errorCount":0,"fatalErrorCount":0,"warningCount":2,"fixableErrorCount":0,"fixableWarningCount":0,"source":"59"},{"filePath":"60","messages":"61","suppressedMessages":"62","errorCount":0,"fatalErrorCount":0,"warningCount":1,"fixableErrorCount":0,"fixableWarningCount":0,"source":"63"},"/Users/julianahyc/Desktop/Senior/SeniorüçÅ/portfolio/src/index.js",[],[],"/Users/julianahyc/Desktop/Senior/SeniorüçÅ/portfolio/src/App.js",["64","65"],[],"import React, { Component } from 'react';\nimport { HashRouter,Routes, Route, Link } from 'react-router-dom';\n// import Home from './pages/Home.js';\n// import Navbar from './pages/Navbar.js';\n// import About from './pages/About.js';\n// import Art from './pages/Art.js';\n// import Persona from './pages/Persona.js';\n// import Responsive from './pages/Responsive.js';\n// import Iterative from './pages/Iterative.js';\n// import Development from './pages/Development.js';\nimport Home from './pages/Home';\nimport Navbar from './pages/Navbar';\nimport About from './pages/About';\nimport Art from './pages/Art';\nimport Persona from './pages/Persona';\nimport Responsive from './pages/Responsive';\nimport Iterative from './pages/Iterative';\nimport Development from './pages/Development';\n\n// source: https://www.geeksforgeeks.org/reactjs-router/\nclass App extends Component {\nrender() {\n\treturn (\n    <div className=\"App\">\n\t<HashRouter>\n      <Navbar/>\n      <Routes>\n          {/* <Route exact path='/CS1300_Portfolio' element={< Home />}></Route>\n          <Route exact path='CS1300_Portfolio/art' element={< Art />}></Route>\n          <Route exact path='CS1300_Portfolio/persona' element={< Persona />}></Route>\n          <Route exact path='CS1300_Portfolio/responsive' element={< Responsive />}></Route>\n          <Route exact path='CS1300_Portfolio/iterative' element={< Iterative />}></Route>\n          <Route exact path='CS1300_Portfolio/development' element={< Development />}></Route> */}\n          <Route path='/' element={< Home />}></Route>\n          <Route path='/art' element={< Art />}></Route>\n          <Route path='/persona' element={< Persona />}></Route>\n          <Route path='responsive' element={< Responsive />}></Route>\n          <Route path='/iterative' element={< Iterative />}></Route>\n          <Route path='/development' element={< Development />}></Route>\n      </Routes>\n\t</HashRouter>\n  </div>\n);\n}\n}\n\nexport default App;\n","/Users/julianahyc/Desktop/Senior/SeniorüçÅ/portfolio/src/pages/Navbar.js",[],[],"/Users/julianahyc/Desktop/Senior/SeniorüçÅ/portfolio/src/pages/Iterative.js",["66","67","68","69","70","71","72","73","74","75","76","77","78","79","80","81","82","83","84","85"],[],"import React from 'react';\nimport './Project.css';\nimport { useEffect } from 'react';\n\nfunction Iterative() {\n  useEffect(() => {\n    window.scrollTo(0, 0)\n  }, [])\n  return (\n    <div className=\"Project\">\n        <div className='back_button'>\n           <a href='/CS1300_Portfolio'><button> &lt;  Back to Projects</button><br/><br/></a> \n        </div>\n        <div className='Project_intro'>\n          <p style={{fontSize:'4rem',display:'inline'}}> Circular  <br/><br/> </p>\n          <p style={{color:'#4682B4',display:'inline'}}><b>Analyzing </b></p>  peer and user feedback on the webpage redesign prototype for a tech start-up to match design goals with actual user experiences.\n        </div>\n\n        <div className='Project_content'>\n        <div className='Project_context'>\n            <div className='section_title'>Context & Aim</div>\n            <p>We‚Äôve designed a web application for <a href=\"https://www.ycombinator.com/companies/circular\" className='responsive_url'>Circular</a>, a startup that aims to democratize technology by allowing users to experience new technology before fully committing ‚Äî users initially rent a product, and if they enjoy it, they can choose to buy the product. </p>\n            <p>We‚Äôve targeted two types of users: ones who are new to the website, and ones who are current users. The website is designed to be comprehensive enough to lead a potential user to explore all of Circular‚Äôs offerings and find a product that they would love to experience. Once a steady base of users has been built up, the website begins to serve a second purpose: to allow current users to manage their current products, continue finding new products to try, and to configure their desires in the most effective way.</p>\n            <p>This is a group project and I am mainly responsible for organizing and analyzing peer and user feedback. Through this project, I aim to gain experience in the iterative design process, with a focus on collecting feedback from design peers and interpreting results from user testing experiments, and eventually actualizing the feedback to changes in the design product to improve <b>user experience</b>.</p>\n            </div>\n\n         <div className='Project_process'>\n            <div className='section_title'>Process</div>\n\n            <div className='section_subtitle'>STEP 1: Ideating through sketches</div>\n                <p>We created 4 different possible designs for circular below, trying to capture a broad range of design alternatives for circular.</p>\n                <p>An attached pdf with our sketches are shown below:</p>\n\n              <iframe className='iframe' src={process.env.PUBLIC_URL + \"/\" + \"images/iterative_sketches.pdf\"}></iframe>\n            <br/><br/>\n\n            <div className='section_subtitle'>STEP 2: Prototyping (Low-Fi)</div>\n            <p>We then developed a series of wireframes integrating the best design ideas from our sketches.</p>\n            <p>We wanted to emphasize that the key idea of Circular is that it is a marketplace with unconventional means of renting the products. Our sketches were made of pages needed to create a functioning marketplace, and from there we created a flow that covered two use cases, first time users and returning customers.</p>\n            <p>Here are some of the ideas that guided our wireframing process:</p>\n            <ul>\n                <li>We decided on having the users first go through a landing page because Circular‚Äôs company mission serves as a guide to how the marketplace works, as well as setting them apart from other marketplaces.</li>\n                <li>A logged out state and logged in state exist for certain pages, to both prevent new users from being overwhelmed with information irrelevant in deciding to join the marketplace, and making the flow of information more efficient to returning users. E.g the explore page showcasing rental information in the logged in state, but is missing in the logged out state.</li>\n                <li>We wanted to make sure users are aware of the actions they can take when renting a rental, so information regarding it was included in multiple areas, such as the Landing page in ‚ÄúHERE‚ÄôS HOW IT WORKS‚Äù and the Product page in ‚ÄúWhat happens then?‚Äù.</li>\n            </ul>\n            <p>An attached pdf with our wireframes are shown below:</p>\n                <iframe className='iframe' src={process.env.PUBLIC_URL + \"/\" + \"images/iterative_wireframe.pdf\"}></iframe>\n            <br/><br/>\n\n    <div className='section_subtitle'>STEP 3: Prototyping (Hi-Fi)</div>\n    <br/>\n    <iframe className='iframe' src={\"https://www.figma.com/embed?embed_host=share&url=https%3A%2F%2Fwww.figma.com%2Ffile%2FOCjo8lVwty9xodJxQsX54j%2FCircular%253A-Initial-High-Fidelity-Mockups%3Fnode-id%3D1%253A99%26t%3D6hUOivWMhTGj7ZBT-1\"}></iframe><br/>\n    <a className='figma_link' href=\"https://www.figma.com/proto/OCjo8lVwty9xodJxQsX54j/Circular%3A-Initial-High-Fidelity-Mockups?node-id=1%3A99&scaling=scale-down&page-id=0%3A1&starting-point-node-id=1%3A99&fbclid=IwAR2tHjtlL-tSsfecCNyjQ0dKjvnCk_WXrvwwNm1wfTG7zdTSAhrbjnf-g80\" target=\"_blank\">Accessing the protoype</a>\n    <br/>\n    <b>Design Considerations</b>\n    <p>We then proceeded to convert our wireframes into high-fidelity prototypes. During this stage, we continued to refine our design with a few key principles in mind:</p>\n    <ul>\n        <li>We decided on an accent color of a bright pinkish red and roundness across our elements to convey a sense of newness, energy, and friendliness against more conventional, wholesale tech retailers, as circular is meant to widen the accessibility of technology through unconventional rentals.</li>\n        <li>We used our accent color sparingly with mostly-white backgrounds to encourage emphasis on the most important elements of each page, in addition to applying hierarchy to text.</li>\n    </ul>\n    <br/>\n\n    <div className='section_subtitle'>STEP 4: Analyzing peer feedback</div>\n        <div id=\"critiques\">\n        <p>After reviewing our feedback, we broke down the issues our peers brought up before designing new solutions based on their suggestions. All of the solutions we describe were then implemented in an updated high-fidelity prototype, featured further down.</p>\n        <div class=\"critique-section\">\n        <div class=\"critique-title\">\n                  <h4 class=\"subtitle\">General</h4>\n                  This section features problems and solutions that span across several pages\n                </div>\n        <div class=\"critique-content\">\n\n                  <div class=\"critique-problems\">\n                    <b>Problems Identified</b>\n                    <div class=\"critique-problem\">\n                      <p>\n                        Information/instructions on how renting works should be more accessible throughout the whole website instead of being on the landing page only\n                      </p>\n                    </div>\n                    <div class=\"critique-problem\">\n                      <p>\n                        Some peers have trouble navigating to the desired page using breadcrumbs; some wish that there is a back button that goes back to the previous page that they were on\n                      </p>\n                    </div>\n\n                  </div>\n\n                  <div class=\"critique-solutions\">\n                    <b>Solutions</b>\n                    <div class=\"critique-solution\">\n                      <p>\n                        Add renting information on the product page\n                      </p>\n                    </div>\n\n                    <div class=\"critique-solution\">\n                      <p>\n                        Gray out the breadcrumb that corresponds to the current page that the user is on\n                      </p>\n                    </div>\n\n                    <div class=\"critique-solution\">\n                      <p>\n                        (We agreed that a back button is not necessary because users generally use the browser‚Äôs back button)\n                      </p>\n                    </div>\n\n                  </div>\n\n\n                </div>\n              </div>\n\n              <div class=\"critique-section\">\n\n                <div class=\"critique-title\">\n                  <h4 class=\"subtitle\">Landing Page</h4>\n                </div>\n\n                <div class=\"critique-content\">\n\n                  <div class=\"critique-problems\">\n                    <b>Problems Identified</b>\n                    <div class=\"critique-problem\">\n                      <p>\n                        This page includes important information such as a brief introduction of the start-up and an explanation of the rental process, but there is no way of going back to this page once users click the ‚ÄúCHOOSE NOW‚Äù button because the logo on the navbar currently links to explore page\n                      </p>\n                    </div>\n\n                    <div class=\"critique-problem\">\n                      <p>\n                        Information on how renting works is very important and should have a higher priority than the product display section\n                      </p>\n                    </div>\n\n                    <div class=\"critique-problem\">\n                      <p>\n                        The wording of the ‚ÄúChoose Now‚Äù button is confusing - peers expect it to lead to a page where they can browse and choose from available products, but it just leads to the exploration page instead\n                      </p>\n                    </div>\n                  </div>\n\n                  <div class=\"critique-solutions\">\n                    <b>Solutions</b>\n                    <div class=\"critique-solution\">\n                      <p>\n                        Reorganize navbar: link the logo to the landing page, add an ‚ÄúExplore‚Äù tab next to the ‚ÄúRentals‚Äù tab\n                      </p>\n                    </div>\n\n                    <div class=\"critique-solution\">\n                      <p>\n                        Put the ‚ÄúHow It Works‚Äù section above the ‚ÄúPopular products‚Äù section\n                      </p>\n                    </div>\n\n                    <div class=\"critique-solution\">\n                      <p>\n                        Change the button from ‚ÄúChoose Now‚Äù to ‚ÄúExplore Now‚Äù\n                      </p>\n                    </div>\n                  </div>\n\n\n                </div>\n\n                \n\n\n              </div>\n\n              <div class=\"critique-section\">\n\n                <div class=\"critique-title\">\n                  <h4 class=\"subtitle\">Explore Page</h4>\n                </div>\n\n                <div class=\"critique-content\">\n\n                  <div class=\"critique-problems\">\n                    <b>Problems Identified</b>\n                    <div class=\"critique-problem\">\n                      <p>\n                        The layout is very different when logged in and when logged out, confusing returning users before they log in\n                      </p>\n                    </div>\n\n                  </div>\n\n                  <div class=\"critique-solutions\">\n                    <b>Solutions</b>\n                    <div class=\"critique-solution\">\n                      <p>\n                        Add a section telling users to log in to see current rentals with a ‚ÄúLog In‚Äù action button, indicating how returning users can view rentals if they log in\n                      </p>\n                    </div>\n\n                  </div>\n\n\n                </div>\n              </div>\n\n              <div class=\"critique-section\">\n\n                <div class=\"critique-title\">\n                  <h4 class=\"subtitle\">Categories Page</h4>\n                </div>\n\n                <div class=\"critique-content\">\n\n                  <div class=\"critique-problems\">\n                    <b>Problems Identified</b>\n\n                    <div class=\"critique-problem\">\n                      <p>\n                        The category icons are unnecessarily big such that the page conveys little information yet requires scrolling\n                      </p>\n                    </div>\n\n                    <div class=\"critique-problem\">\n                      <p>\n                        Lacks hierarchy - the three featured categories are not emphasized enough\n                      </p>\n                    </div>\n\n                    <div class=\"critique-problem\">\n                      <p>\n                        Categories are arbitrarily sorted\n                      </p>\n                    </div>\n\n                  </div>\n\n                  <div class=\"critique-solutions\">\n                    <b>Solutions</b>\n                    <div class=\"critique-solution\">\n                      <p>\n                        Shrink the icons and fit more icons in one row\n                      </p>\n                    </div>\n\n                    <div class=\"critique-solution\">\n                      <p>\n                        Invert the color of the three curated categories to emphasize them</p>\n                    </div>\n\n                    <div class=\"critique-solution\">\n                      <p>\n                        Sort the categories by popularity </p>\n                    </div>\n\n                  </div>\n\n\n                </div>\n              </div>\n\n              <div class=\"critique-section\">\n\n                <div class=\"critique-title\">\n                  <h4 class=\"subtitle\">Product Page</h4>\n                </div>\n\n                <div class=\"critique-content\">\n\n                  <div class=\"critique-problems\">\n                    <b>Problems Identified</b>\n                    <div class=\"critique-problem\">\n                      <p>\n                        Information on how renting works is too short, more explanation is needed\n                      </p>\n                    </div>\n\n                  </div>\n\n                  <div class=\"critique-solutions\">\n                    <b>Solutions</b>\n                    <div class=\"critique-solution\">\n                      <p>\n                        Add a section explaining the rental process; include a link next to where users select the rental period that scrolls to this section\n                      </p>\n                    </div>\n\n                  </div>\n\n\n                </div>\n              </div>\n\n              <div class=\"critique-section\">\n\n                <div class=\"critique-title\">\n                  <h4 class=\"subtitle\">My Rentals Page (feedback from guest critic)</h4>\n                </div>\n\n                <div class=\"critique-content\">\n\n                  <div class=\"critique-problems\">\n                    <b>Problems Identified</b>\n                    <div class=\"critique-problem\">\n                      <p>\n                        Users should be able to know immediately what they can do and what options they have about the current rentals, especially the option to end the rental, so they do not feel anxious about paying money for so many products\n                      </p>\n                    </div>\n\n                  </div>\n\n                  <div class=\"critique-solutions\">\n                    <b>Solutions</b>\n                    <div class=\"critique-solution\">\n                      <p>\n                        Change layout from two products per row to one product per row to include more information\n                      </p>\n                    </div>\n\n                    <div class=\"critique-solution\">\n                      <p>\n                        Add three action buttons for each rental\n                      </p>\n                    </div>\n\n                  </div>\n\n\n                </div>\n              </div>\n\n              <div class=\"critique-section\">\n\n                <div class=\"critique-title\">\n                  <h4 class=\"subtitle\">Rental Details Page (feedback from guest critic)</h4>\n                </div>\n\n                <div class=\"critique-content\">\n\n                  <div class=\"critique-problems\">\n                    <b>Problems Identified</b>\n                    <div class=\"critique-problem\">\n                      <p>\n                        The layout looks too similar to the shopping product page and users might get confused about whether it is a product that they are about to rent or a product that they are currently renting\n                      </p>\n                    </div>\n\n                    <div class=\"critique-problem\">\n                      <p>\n                        More information, such as the cumulative amount paid, the last payment date, etc. should be available\n                      </p>\n                    </div>\n\n                    <div class=\"critique-problem\">\n                      <p>\n                        The three options (extend, buy, end) should be emphasized and placed next to relevant information such as corresponding dates and prices \n                      </p>\n                    </div>\n\n                  </div>\n\n                  <div class=\"critique-solutions\">\n                    <b>Solutions</b>\n                    <div class=\"critique-solution\">\n                      <p>\n                        Redesign the layout to differentiate from the shopping product page (smaller photo, less product information, more information on next steps)\n                      </p>\n                    </div>\n\n                    <div class=\"critique-solution\">\n                      <p>\n                        Include information: shipping, payment method, address, upcoming payments, etc.\n                      </p>\n                    </div>\n\n                    <div class=\"critique-solution\">\n                      <p>\n                        Use three cards corresponding to three actions, each with information and an action button\n                      </p>\n                    </div>\n\n                  </div>\n                  </div>\n                  </div>\n                </div>\n\n                <br/><br/>\n\n                <div className='section_subtitle'>STEP 5: Updating Hi-Fi prototype</div>\n                <p>\n                After our critique feedback, we then implemented all the above solutions towards a revision of our initial mockup.\n              </p>\n                <iframe className='iframe' src={\"https://www.figma.com/embed?embed_host=share&url=https%3A%2F%2Fwww.figma.com%2Ffile%2FK7SHNKZf5nZClK20VV4FFr%2FCircular%253A-Updated-High-Fidelity-Mockups%3Fnode-id%3D1%253A98%26t%3DqSttu9A9EuBSZqen-1\"}></iframe><br/>\n                <a className='figma_link' href=\"https://www.figma.com/proto/K7SHNKZf5nZClK20VV4FFr/Circular%3A-Updated-High-Fidelity-Mockups?node-id=1%3A98&scaling=scale-down&page-id=0%3A1&starting-point-node-id=1%3A98&fbclid=IwAR0_rRgceDHtxL_gZPN3l1jpFmt0Uz7SB9XVXeVftDZ4612h5drFGQn9pN8\" target=\"_blank\">Accessing the protoype</a>\n                <br/> <br/>\n\n                <div className='section_subtitle'>STEP 6: Analyzing user testing results</div>\n                <p><b>Methodology</b></p>\n                <p>For user testing, we first identified the two main functionalities of the interface that we would want users to explore:</p>\n                <ol>\n                    <li>To start new rentals</li>\n                    <li>To manage current rentals</li>\n                </ol>\n                <p>These resulted in the following introduction prompt for users testing our mockup:</p>\n                <p><b>\"This is an interactive mockup website made on prototyping software. The website allows users to try out tech products before either buying or returning them. Imagine you're looking to buy and try out some new electronics.\"</b></p>\n                <p>We then set up 4 sub-tasks that guide the users through the user flow:</p>\n                <ol>\n                    <li>Check out available categories</li>\n                    <li>Select a category, browse the products, and select a specific product</li>\n                    <li>Read about how renting works</li>\n                    <li>Buy a product that they are currently renting</li>\n                </ol>\n                <p>After each sub-task, we also asked our users the following:</p>\n                <ol>\n                    <li>To describe whether they felt they successfully completed the subtask</li>\n                    <li>To rate the difficulty of the subtask from 1 to 5 (1 = Very Difficult, 5 = Very Easy)</li>\n                </ol>\n                <p><b>Task Breakdown</b></p>\n                <p>For each of the 4 sub-tasks, you can find the task description, our expectation on how the user would complete the task, and a summary of what each user actually did and their responses to the post-task questions.</p>\n                <p>Furthermore, we have marked, with a number (e.g. \"(1)\"), when a specific user's experience relates with a potential change we came up with after looking over our data, covered in the next section.</p>\n                \n                <div className='subtasks'>\n                <div class=\"subtask\">\n\n                    <div class=\"subtask-title\">\n                    <h4 class=\"subtitle\">Subtask 1</h4>\n                    </div>\n\n\n                    <div class=\"subtask-description\">\n                    <p>\n                        <b>Prompt: </b>\n                        Imagine you want to find out what types of products the website offers. Now use this website to find this information. Stop when you feel like you have seen a complete list of what products are available.\n                    </p>\n                    </div>\n\n                    <div class=\"subtask-expected\">\n                    <p>\n                        <b>Expected Taskflow: </b>\n                        Click the ‚ÄúExplore now‚Äù button on the landing page to go to the explore page; see the selected categories and click the ‚ÄúView all categories‚Äù below.\n                    </p>\n                    </div>\n\n                    <div class=\"subtask-results\">\n\n                    <div class=\"subtask-result subtask-result-a\">\n                        <b>User A - Completed</b>\n                        <p>First action was clicking the search bar, then used breadcrumbs to go to the correct categories page\n                    \n                        (Reported they were sure that they completed the task successfully, and rated it 4/5.)\n                        </p>\n                    </div>\n\n                    <div class=\"subtask-result subtask-result-b\">\n                        <b>User B - Completed</b>\n                        <p>Navigated to the correct categories page but did not recognize it as the correct page, then went back to the explore page\n                    \n                        (Reported they were sure that they completed the task successfully, and rated it 5/5.)\n                        </p>\n                        \n                    </div>\n\n                    <div class=\"subtask-result subtask-result-c\">\n                        <b>User C - <i>Incomplete</i></b>\n                        <p>Thought the categories on the explore page were a complete list, did not see the ‚ÄúView all categories‚Äù link (1)\n\n                        (Reported they were sure that they completed the task successfully, and rated it 4/5.)\n                        </p>\n                    </div>\n\n                    </div>\n\n                    </div>\n\n                    <div class=\"subtask\">\n\n                    <div class=\"subtask-title\">\n                    <h4 class=\"subtitle\">Subtask 2</h4>\n                    </div>\n\n\n                    <div class=\"subtask-description\">\n                    <p>\n                        <b>Prompt: </b>\n                        Imagine that you are looking to try out a new tablet. Try to browse the tablet section and then rent a tablet using the website. Go as far as you can with the prototype before moving on to the next task.\n                    </p>\n                    </div>\n\n                    <div class=\"subtask-expected\">\n                    <p>\n                        <b>Expected Taskflow: </b>\n                        Click the tablet icon on the categories page to go to the browse page, then click the image or the title of a product to go to the product page.\n                    </p>\n                    </div>\n\n                    <div class=\"subtask-results\">\n\n                    <div class=\"subtask-result subtask-result-a\">\n                        <b>User A - <i>Incomplete</i></b>\n                        <p>Clicked the rental button on the browse page but did not check product specifics (2)\n                        (Reported they were sure that they completed the task successfully, and rated it 5/5.)\n                        </p>\n                    </div>\n\n                    <div class=\"subtask-result subtask-result-b\">\n                        <b>User B - <i>Incomplete</i></b>\n                        <p>Clicked the rental button on the browse page but did not check product specifics (2)\n\n                        (Reported they were sure that they completed the task successfully, and rated it 5/5.)\n                        </p>\n                    </div>\n\n                    <div class=\"subtask-result subtask-result-c\">\n                        <b>User C - Completed</b>\n                        <p>Had some trouble finding the tablet category but completed successfully, only realized that this was a charged rental service instead of free trials until seeing the ‚Äúrent‚Äù button (3)\n                    \n                        (Reported they were NOT sure that they completed the task successfully, and rated it 3/5.)\n                        </p>\n                    </div>\n\n                    </div>\n\n                    </div>\n\n                    <div class=\"subtask\">\n\n                    <div class=\"subtask-title\">\n                    <h4 class=\"subtitle\">Subtask 3</h4>\n                    </div>\n\n\n                    <div class=\"subtask-description\">\n                    <p>\n                        <b>Prompt: </b>\n                        Imagine you want to rent this tablet but you are confused about how the renting process works. Try to find more information about what you will need to do after trying out a product. Stop when you think you have found the corresponding section.\n                    </p>\n                    </div>\n\n                    <div class=\"subtask-expected\">\n                    <p>\n                        <b>Expected Taskflow: </b>\n                        At the product page, click ‚ÄúHow does renting work?‚Äù or scroll down to the section.\n                    </p>\n                    </div>\n\n                    <div class=\"subtask-results\">\n\n                    <div class=\"subtask-result subtask-result-a\">\n                        <b>User A - Completed</b>\n                        <p>Looked for information on the browse page at first before going to the correct product page (3)\n                    \n                        (Reported they were sure that they completed the task successfully, and rated it 4/5.)\n                        </p>\n                        \n                    </div>\n\n                    <div class=\"subtask-result subtask-result-b\">\n                        <b>User B - <i>Incomplete</i></b>\n                        <p>Looked for a separate ‚Äúabout‚Äù or ‚Äúterms & conditions‚Äù page and ended with the ‚ÄúHow it works‚Äù section on the landing page (3)\n\n                        (Reported they were NOT sure that they completed the task successfully, and rated it 5/5.)\n                        </p>\n                    </div>\n\n                    <div class=\"subtask-result subtask-result-c\">\n                        <b>User C - Completed</b>\n                        <p>Already on the product page and found the correct section, but asked for a more detailed explanation\n\n                        (Reported they were sure that they completed the task successfully, and rated it 4/5.)\n                        </p>\n                    </div>\n\n                    </div>\n\n                    </div>\n\n                    <div class=\"subtask\">\n\n                    <div class=\"subtask-title\">\n                    <h4 class=\"subtitle\">Subtask 4</h4>\n                    </div>\n\n\n                    <div class=\"subtask-description\">\n                    <p>\n                        <b>Prompt: </b>\n                        Imagine you rented a pair of Sennheiser headphones to try out from this website a while ago. Now use this website to see how much it would cost to buy the headphones now.\n                    </p>\n                    </div>\n\n                    <div class=\"subtask-expected\">\n                    <p>\n                        <b>Expected Taskflow: </b>\n                        Click the ‚ÄúRentals‚Äù tab on the navbar to go to the manage rentals page then the ‚Äúview rental‚Äù button, or ‚ÄúYour rentals‚Äù section on the explore page, to go to the rental details pages, find the corresponding section.\n                    </p>\n                    </div>\n\n                    <div class=\"subtask-results\">\n\n                    <div class=\"subtask-result subtask-result-a\">\n                        <b>User A - Completed</b>\n                        <p>Had some confusion understanding the task but completed successfully\n\n                        (Reported they were sure that they completed the task successfully, and rated it 4/5.)\n                        </p>\n                    </div>\n\n                    <div class=\"subtask-result subtask-result-b\">\n                        <b>User B - Completed</b>\n                        <p>First clicked the ‚ÄúYour rentals‚Äù under the account drop-down menu, which was not implemented\n\n                        (Reported they were sure that they completed the task successfully, and rated it 4/5.)\n                        </p>\n                    </div>\n\n                    <div class=\"subtask-result subtask-result-c\">\n                        <b>User C - Completed</b>\n                        <p>Asked for a more detailed explanation of the price (4)\n                        (Reported they were sure that they completed the task successfully, and rated it 4/5.)\n                        </p>\n                    </div>\n\n                    </div>\n                    </div>\n                    </div>\n                    <br/>\n                    \n                    <p><b>Overall analysis</b></p>\n                    <p>Overall, the three users were able to complete the tasks successfully and through the expected task flows. Most of their confusion came either from some incomplete functionalities (e.g. dummy buttons) of the interface as a prototype, or from how the task descriptions were worded. At the same time, some of their confusion does reflect certain problems with our interface, and we have come up with a list of potential changes in response to these problems as well as the users‚Äô oral feedback.</p>          \n                    <br/>\n                    \n                    <div className='section_subtitle'>STEP 7: Looking forward</div>\n                    <p>Based on our results, we came up with a list of proposed changes to address the issues we found during user testing. We have marked, in the previous section, how specific results from our user testing link to our proposed changes.</p>\n                    <ol>\n                  <li>\n                    Change the wording of ‚Äúview all categories‚Äù to ‚Äúview more categories‚Äù and increase visibility with the scale of view button on the explore page\n                  </li>\n                  <li>\n                    Make the whole card for a product clickable instead of only the image and the title\n                  </li>\n                  <li>\n                    Add more information on how renting works throughout the website\n                    <ul>\n                      <li>\n                        Change the wording on the landing page from ‚Äútry‚Äù to ‚Äúrent‚Äù\n                      </li>\n                      <li>\n                        Include a question mark icon on the browse page explaining the renting process\n                      </li>\n                      <li>\n                        Include a separate ‚Äúabout‚Äù page, with a button displayed on the top nav bar\n                      </li>\n                    </ul>\n                  </li>\n                  <li>\n                    Provide infographic/texts on buy price breakdown and how the buy price changes based on months rented (e.g. price if buy next month, how this month‚Äôs rent is calculated)\n                  </li>\n                  <li>\n                    (General) Add more intended functionality to our prototype\n                    <ul>\n                      <li>\n                        Make the ‚ÄúYour Rentals‚Äù button in account dropdown menu link to the manage rentals page\n                      </li>\n                      <li>\n                        Make ‚ÄúRent‚Äù, ‚ÄúBuy‚Äù, ‚ÄúExtend‚Äù, and ‚ÄúEnd‚Äù buttons lead to new functional pages linked to those actions\n                      </li>\n                    </ul>\n                  </li>\n\n                </ol>\n              </div>\n\n\n     \n\n         <div className='Project_deliverables'>\n            <div className='section_title'>deliverables</div>\n            <div className='section_subtitle'>User testing results</div>\n            <br/>\n            <div class=\"gifs\">\n\n                <div class=\"gif\">\n                  <img src={process.env.PUBLIC_URL + \"/\" + \"images/GifA.gif\"}></img><br/>\n                  Preview of User A: &nbsp; &nbsp;\n                  <a href={process.env.PUBLIC_URL + \"/\" + \"images/UserTestingA.mp4\"} target=\"_blank\" download>Click here to download User A's experience</a>\n                </div>\n                <br/>\n\n                <div class=\"gif\">\n                  <img src={process.env.PUBLIC_URL + \"/\" + \"images/GifB.gif\"}></img><br/>\n                  Preview of User B: &nbsp; &nbsp;\n                  <a href={process.env.PUBLIC_URL + \"/\" + \"images/UserTestingB.mp4\"}  target=\"_blank\" download>Click here to download User B's experience</a>\n               \n                </div>\n                <br/>\n\n                <div class=\"gif\">\n                  <img src={process.env.PUBLIC_URL + \"/\" + \"images/GifC.gif\"}></img><br/>\n                    Preview of User C: &nbsp; &nbsp;\n                  <a href={process.env.PUBLIC_URL + \"/\" + \"images/UserTestingC.mp4\"}  target=\"_blank\" download>Click here to download User C's experience</a>\n                </div>\n                <br/>\n\n                </div>\n        </div>\n\n         <div className='Project_takeaways'>\n            <div className='section_title'>Takeaways</div>\n            <ul style={{lineHeight:'2.8rem'}}>\n                    <li>I learned about the workflow of the iterative design process, including sketching ideas, creating low-fidelity and high-fidelity prototypes, collecting peer feedback, revising the design, and finally conducting user testings and further revising the design.</li>\n                    <li>I gained experience in working in a design team and learned about how to brainstorm and then gather design ideas, how to express my own ideas while understanding others, how to distribute tasks, etc. Communication is the key!</li>\n                    <li>Since my work focused on analyzing the feedback, it is interesting seeing how users' actual interactions with our prototype differed from our design goals and expectations. </li>\n                    <li>Never assume that there is a \"right\" way of using a design product - always center around the users' experience!</li>\n                    <li>(We were so excited when we got the email from the start-up saying that our design was forwarded to the product design team! But it is sad that we never heard back since... ;( )</li>\n                </ul>\n                <br/><br/><br/><br/>\n    \n         </div>\n        </div>\n\n        <div className='footer'> Juliana & Shaun 2022<br/>Hand-coded with &#9829;</div>\n\n    </div>\n  );\n}\n\nexport default Iterative;\n","/Users/julianahyc/Desktop/Senior/SeniorüçÅ/portfolio/src/pages/Responsive.js",["86","87","88","89","90","91","92","93","94","95","96","97","98","99","100","101","102","103","104","105","106"],[],"import React from 'react';\nimport './Project.css';\nimport { useEffect } from 'react';\n\nfunction Responsive() {\n    useEffect(() => {\n        window.scrollTo(0, 0)\n      }, [])\n  return (\n    <div className=\"Project \">\n        <div className='back_button'>\n           <a href='/CS1300_Portfolio'><button> &lt;  Back to Projects</button><br/><br/></a> \n        </div>\n        <div className='Project_intro'>\n          <p style={{fontSize:'4rem',display:'inline'}}> HENG Thai Rotisserie  <br/><br/> </p>\n          <p style={{color:'#4682B4',display:'inline'}}><b>Experimenting </b></p> ways to improve a local restaurant's webpage by identifying current problems and enhancing visual and logical organization.\n            \n        </div>\n\n        <div className='Project_content'>\n        <div className='Project_context'>\n            <div className='section_title'>Context & Aim</div>\n                 <p>In this project, I redesigned the home webpage of Heng Thai & Rotisserie, a popular (and personal favorite!) Thai restaurant on college hill. By evaluating the webpage against the following criteria of usability, learnability, and memorability as well as with <a className=\"responsive_url\" href=\"https://wave.webaim.org/\">WebAIM WAVE</a>, a web accessibility evaluation tool, I was able to identify existing problems with the webpage. </p>\n                 <p>In response to these problems, I redesigned the webpage with lo-fi prototypes, high-fi prototypes, and a visual design style guide, and finally implemented the redesign into a responsive webpage with suitable layouts for the varying mobile, tablet, and desktop screen sizes.</p>\n                <p>Through this project, I aim to focus on reorganizing the existing webpage to present information in a more visually and logically cohesive way. I believe that clarity in structure is the most important element for a page conveying a large amount of information for the users to easily navigate and hence improve <b>user experience</b>.</p>\n            </div>\n\n         <div className='Project_process'>\n            <div className='section_title'>Process</div>\n\n            <div className='section_subtitle'>STEP 1: Identifying usability problems</div>\n                <p>Click <a className=\"responsive_url\" href=\"https://www.hengpvd.com/home\">HERE</a> to view the original website. </p>\n                <p>This HOME page of the restaurant includes information on hours, how to order and make reservations, contact information, and images to showcase Thai streetfood.</p>\n                <div className= 'responsive_half'>\n                <div>\n                    <b>Usability</b>\n                    <ul class=\"sublist\">\n                        <li>The sequence of information is confusing - information on open hours is sandwiched by instructions to make a reservation or make an order</li>\n                        <li>Open hours are center aligned, making it difficult to separate weekdays and hours</li>\n                        <li>Information about open hours is conveyed in two ways. <br/> The top one says ‚Äú11.30 pm ‚Äì 3.00 pm and 4.30 pm ‚Äì 9.30 pm‚Äù, whereas the bottom one (in the VIST US section) says ‚Äú11.30 AM ‚Äì 9.30 PM, ‚Ä¶, We‚Äôre closed for a break from 3.00 ‚Äì 4.30 PM everyday‚Äù. Users might spend some time and mental energy comparing these blocks of information</li>\n                        <li>Inconsistency in information - no instruction on how to use \"curbside service\"</li>\n\n                        <li>The purpose of the contact us form is unclear</li>\n                    </ul>\n                <b>Learnability</b>\n                    <ul class=\"sublist\">\n                        <li> Poor organization of the top nav bar - menus are not grouped together, and the important contact information is hidden is \"MORE\"</li>\n                        <li>No section title or sub-menus, difficult for users to learn about what information is included on this HOME page</li>\n                        <li>The in-page navigation buttons on the right do work, but no indication of which section it links to</li>\n                        <li>The label for the first button just says ‚ÄúOnline Order‚Äù but does not specify what type of order (pick-up or delivery), so users have to click the button to check </li>\n                    </ul>\n                <b>Memorability</b>\n                    <ul class=\"sublist\">\n                        <li>Repetition in information about open hours, both on the first page and in the VISIT US section at the bottom of the page, makes it difficult for users to remember which section to check for hours</li>\n                    </ul>\n\n                <b>Styling</b>\n                <ul class=\"sublist\">\n                    <li>The grey block in the center does not align with the background image</li>\n                    <li>Two buttons have different formats and do not align </li>\n                    <li>Inconsistency in typography ‚Äì three slogans with three different fonts</li>\n                    <li>The footnote takes up a whole page</li>\n                </ul>\n                <b>Accessibility - WAVE results</b>\n                <ul class=\"sublist\">\n                    <li>Contrast errors from the grey block and the white texts on top. <br/> - I do not think the contrast is too low, but I would consider using larger font sizes.</li>\n                    <li>Contrast errors from the \"DELIVERY\" button with yellow background and blue texts. <br/> - I agree that the contrast is very low, epescially in grayscale.</li>\n                    <li> Redudant links from the in-page navigation buttons on the right. <br/> - I think it is important to have some anchors on one page, but I do agree that these buttons should be labeled with the section on the page that they link to.</li>\n                    <li> Good ARIA functionality. <br/> - I believe that this page was developed with an online template, and I agree that the accessibility functionality is good.</li>\n                 </ul>\n                </div>\n\n                <div className='responsive_problem'>\n                    <img src={process.env.PUBLIC_URL + \"/\" + \"images/website1.jpeg\"}></img>\n                    <img src={process.env.PUBLIC_URL + \"/\" + \"images/website2.jpeg\"}></img>\n                </div>\n            </div>\n            <br/>\n\n            <div className='section_subtitle'>STEP 2: Prototyping (Low-Fi)</div>\n               <p>I created wireframes using Balsamiq as the low fidelity prototypes for logical oragnization.</p>\n               <b>Desktop version:</b><br/>\n               <div className='responsive_wireframe'>\n                    <img src={process.env.PUBLIC_URL + \"/\" + \"images/lowfi1.jpeg\"}></img>\n                    <img src={process.env.PUBLIC_URL + \"/\" + \"images/lowfi2.jpeg\"}></img>\n               </div>\n               <br/>\n               <b>Tablet version:</b><br/>\n               <img className='responsive_img' src={process.env.PUBLIC_URL + \"/\" + \"images/lowfi3.jpeg\"}></img><br/>\n               <b>Mobile version:</b><br/>\n               <img className='responsive_img' src={process.env.PUBLIC_URL + \"/\" + \"images/lowfi4.jpeg\"}></img>\n            <br/>   \n\n    <div className='section_subtitle'>STEP 3: Creating visual style guide</div>\n        <br/>\n        <img className='responsive_img' src={process.env.PUBLIC_URL + \"/\" + \"images/guide.jpeg\"}></img>\n        <br/>\n\n    <div className='section_subtitle'>STEP 4: Prototyping (Hi-Fi)</div>\n    <p>I created a mockup website using Figma as the high fidelity prototypes for visual oragnization.</p>\n        <b>Desktop version:</b><br/>\n        <img className='responsive_hifi' src={process.env.PUBLIC_URL + \"/\" + \"images/highweb.jpeg\"}></img><br/>\n        <b>Tablet version:</b><br/>\n        <img className='responsive_hifi' src={process.env.PUBLIC_URL + \"/\" + \"images/hightablet.jpeg\"}></img><br/>\n        <b>Mobile version:</b><br/>\n        <img className='responsive_hifi' src={process.env.PUBLIC_URL + \"/\" + \"images/highphone.jpeg\"}></img>\n     </div>\n\n         <div className='Project_deliverables'>\n            <div className='section_title'>deliverables</div>\n            <p> Check out my redesign: <a className=\"responsive_url\" href=\"https://sprintingsloth420.github.io/CS1300_redesign/\"> HOME | hengpvd</a>!</p>\n            <iframe className='iframe' src={\"https://sprintingsloth420.github.io/CS1300_redesign/\"}></iframe>\n            <br/>\n        </div>\n\n         <div className='Project_takeaways'>\n            <div className='section_title'>Takeaways</div>\n            <ul style={{lineHeight:'2.8rem'}}>\n                    <li>I learned about the workflow of redesigning a simple website: identifying problems, experimenting with low-fi and high-fi prototypes, and then creating the webpage.</li>\n                    <li>I gained familiarity with a variety of design tools: Balsamiq, Figma, and responsive HTML elements such as flexbox.</li>\n                    <li>I discovered my expertise as a designer - I usually pay extra attention to the \"flow\" of a website, and I ask questions like \"does it have a smooth overall logical flow\" and \"does this part of the content belong here\". I hence focus on organizing information in a logical way, with visual elements to reinforce the structure.</li>\n                    <li>(It is my first time hand-coding a complete website. The experience is invaluable, but the struggle with flexboxes [AHHH...] and just HTML in general is TOO real ;) )</li>\n                </ul>\n                <br/><br/><br/><br/>\n         </div>\n        </div>\n\n        <div className='footer'> Juliana 2022<br/>Hand-coded with &#9829;</div>\n\n    </div>\n  );\n}\n\nexport default Responsive;\n","/Users/julianahyc/Desktop/Senior/SeniorüçÅ/portfolio/src/pages/Persona.js",["107","108","109","110","111","112","113","114"],[],"import React from 'react';\nimport './Project.css';\nimport { useEffect } from 'react';\n\nfunction Persona() {\n  useEffect(() => {\n    window.scrollTo(0, 0)\n  }, [])\n  return (\n    <div className=\"Project\">\n        <div className='back_button'>\n           <a href='/CS1300_Portfolio'><button> &lt;  Back to Projects</button><br/><br/></a> \n        </div>\n        <div className='Project_intro'>\n          <p style={{fontSize:'4rem',display:'inline'}}> Coffee Vending Machine <br/><br/> </p>\n          <p style={{color:'#4682B4',display:'inline'}}><b>Understanding </b></p> different user journeys by creating personas and a storyboard for the offee vending machine located at a Brown library.\n        </div>\n\n        <div className='Project_content'>\n\n        <div className='Project_context'>\n            <div className='section_title'>Context & Aim</div>\n                <p>In this project, I conducted user research on the coffee vending machine located at the Brown University Rockefeller Library. I observed real users' interactions with this selected interface, created relevant interview questions, and conducted interview with some of the users about their experiences. Based on the data collected, I then created two user personas with the four-quadrant empathy maps as well as a storyboard to depict the user journey.</p>\n                <p>Through this project, I aim to really step into a user's shoes to emphasize with the users. I believe that a deep understanding of the user journey is the crucial first step in identifying design issues, tackling these issues, and improving <b>user experience</b>.</p>\n         </div>\n\n         <div className='Project_process'>\n            <div className='section_title'>Process</div>\n\n            <div className='section_subtitle'>STEP 1: Understanding the interface</div>\n            <p>The coffee vending machine has an interactive digital screen to guide the user to purchase a selection of drinks (e.g. coffee, hot chocolate, etc.). It is an automatic vending machine that does not require the labor of a barista and operates 24/7. </p>\n            <p>Here is a sketch of the interface:</p>\n            <div className= 'persona_sketch'>\n            <img src={process.env.PUBLIC_URL + \"/\" + \"images/machine_sketch.png\"}></img>\n                <ol>\n                <li><b>The interactive digital screen. </b> <br/>\n                It displays the following series of screens (check out the storyboard for rough sketches of each screen!):\n                <ul>\n                    <li>Home page with a ‚ÄúTOUCH TO START‚Äù button</li>\n                    <li>Menu & prices </li>\n                    <li>Confirmation page with total price, payment options, and a pop-up button saying ‚ÄúMAKE MY DRINK‚Äù</li>\n                    <li>‚ÄúPREPARING YOUR DRINK‚Äù page </li>\n                    <li>\"ENJOY YOUR DRINK‚Äù page with a video of removing the cup</li>\n                </ul>\n                </li>\n                <li><b>The payment section.</b> <br/>\n                It includes a credit card swipe slot, a bill acceptor, a coin insert, and a contactless payment area. Everything is labeled with an instruction image except the coin insert.\n                </li>\n                <li><b>The pick-up port.</b> <br/>\n                When the machine starts to make a drink, a cup automatically drops down and is held in the air by the clamp. When the drink is done, the cup hangs there until the user pulls the cup out.\n                </li>\n                <li><b>The counter. </b> <br/>\n                It is right next to the machine where users can temporarily place their drinks. There is also a holder for cup lids.\n    \n                </li>\n            </ol>\n            </div>\n            <br/>\n\n            <div className='section_subtitle'>STEP 2: Observing user behaviors</div>\n                <p>I observed 5 users who interacted with the interface and purchased drinks at the machine. Here are some objective observations of user behaviors:</p>\n\n            <div className='persona_interview'>\n                <div> <b>Common behaviors</b>\n                <ul>\n                    <li>Once users approached the machine, they immediately tapped the screen and started interacting with the machine</li>\n                    <li>The majority of users observed interacting with the machine with no or very few pauses</li>\n                    <li>They were all holding a credit card or a phone for Apple Pay throughout the whole purchase experience</li>\n                    <li>The majority of users observed were not paying attention to the machine when the drink was made but instead were on their phones, chatting with friends, or using the nearby vending machines for snacks</li>\n                    <li>All users were staring at the cup and walking slowly after getting their cup full of hot coffee</li>\n                </ul></div>\n\n                <div> <b>Individual behaviors</b>\n                <ul>\n                    <li>User A was looking for something on the counter near the lid holder as well as the cabinets and drawers below</li>\n                    <li>User A got their own bottle from the bag, placed it below the pick-up port, and removed their own bottle when the machine started preparing the drink and a paper cup dropped out from the port. They then tried to get the cup out by wiggling it but stopped doing it when there was the sound of liquid dripping</li>\n                    <li>User B swiped their credit card several times before the machine recognized their payment</li>\n                    <li>User C was holding their own mug in one hand and the paper cup with the drink in the other hand</li>\n                    <li>User C said, ‚Äúit‚Äôs so hot.‚Äù</li>\n                </ul></div>\n            </div>\n            <br/>   \n\n    <div className='section_subtitle'>STEP 3: Interviewing the users</div>\n      <p>Out of the 5 users I observed, I conducted interviews with 3 users.</p>\n\n    <div className='persona_interview'>\n        <div><b>Questions</b>\n        <ol>\n          <li>Are you studying at the library?</li>\n          <li>Do you usually use vending machines? </li>\n          <li>Is this your first time using this coffee vending machine?</li>\n          <li>If yes: Did you experience any difficulty in understanding what to do? If no: How was your first experience like? Did things get easier as you use it more? </li>\n          <li>Why did you choose to use this machine?</li>\n          <li>If there is an open caf√© that is within a 5-minute walk, would you choose to walk and get your drinks there or still use this machine?</li>\n          <li>How was your experience with navigating the screen?</li>\n          <li>How was the payment process?</li>\n          <li>Would you prefer to use your own mug? And why is that?</li>\n          <li>Is there any other comment you‚Äôd want to mention about your experience?</li>\n          <li>Would you use this machine again?</li>\n        </ol></div>\n        <div> <b>Responses</b>\n        <ul>\n          <li>All interviewees were studying at the library and liked the fact that the machine was right here and would hence keep using the machine in the future, even when nearby caf√©s are available.</li>\n          <li>All interviewees had experience using other vending machines and generally agreed that navigating on the digital screen was relatively straightforward. Some pointed out there were too many options, which was a bit confusing.</li>\n          <li>All interviewees indicated that they had some trouble understanding how the machine and the interface worked the first time using it. There was no overview of what they needed to do so they had to try things out along the way, e.g. knowing whether the machine would provide cups, or knowing when to pay.</li>\n          <li>All interviewees pointed out that they had some difficulty with the payment process, either with swiping credit cards or using wrinkled bills.</li>\n          <li>All interviewees preferred having the option of using single-use paper cups or their own mugs - mugs are eaiser to carry around and has better heat insulation, whereas cups are for cases when they don't have own mugs with them.</li>\n        </ul></div>\n\n       \n    \n    </div>\n         </div>\n\n         <div className='Project_deliverables'>\n            <div className='section_title'>deliverables</div>\n            <div className='section_subtitle'>Personas</div>\n            <div className='persona_sketch'>\n                <img src={process.env.PUBLIC_URL + \"/\" + \"images/persona1.png\"}></img>\n                <div><b>Confused Anna</b>, a retired lady who is not very tech-savvy. She enjoys reading at the Rock a lot, and she just finds out about the new coffee vending machine in the lobby and would like a cup of coffee to warm herself up.<br/><br/>\n                     Anna represents the group of users who are not familiar with the machine, possibly because they are first-time users or because they have little experience in using automatic vending machines in general. They are confused about what the whole process is like and what they are supposed to do at each stage. They reflect affordance problems of the interface:<br/>\n                    <ul> <li>Lack of an overview of the whole process <br/></li>\n                    <li>Instructions for each step are scattered around the screen <br/></li>\n                    <li> No clear instruction on when the payment should be made<br/></li>\n                    <li>No clear instruction on whether cups will be provided by the machine, and the lids on the counter can mislead users to search for cups on the counter too <br/></li>\n               </ul>\n                    </div>\n            </div>\n            <br/>\n            <div className='persona_sketch'>\n                 <div>\n                    <b>Grinding Alex</b>, a Brown student concentrating in Psychology. He is pulling an all-nighter for a paper due the next morning and decides to get coffee at 2 am.<br/><br/>\n                    Alex represents more regular users who have experience using this coffee machine so they have little trouble performing normal interactions. However, users who buy coffee at a library are likely to be mentally tired from academic work (or \"grinding\"). They hence reflect more nuanced problems that can further improve the usability of the interface once fixed:<br/>\n                    <ul>\n                    <li>Many options are offered but their differences are not shown<br/></li>\n                    <li>No way for the users to directly observe how full the cup is unless they pull the cup out <br/></li>\n                    </ul>\n\n                 </div>\n                 <img src={process.env.PUBLIC_URL + \"/\" + \"images/persona2.png\"}></img>\n            </div>\n            \n            <br/><br/>\n\n            <div className='section_subtitle'>Storyboard</div>\n            <img id='persona_storyboard' src={process.env.PUBLIC_URL + \"/\" + \"images/storyboard.jpg\"}persona_storyboard></img>\n        </div>\n\n         <div className='Project_takeaways'>\n            <div className='section_title'>Takeaways</div>\n                <ul style={{lineHeight:'2.8rem'}}>\n                    <li>This is my first taste of user experience research. Important first step!</li>\n                    <li>Even when the users are interacting with the exact same interface, there are very different ways of interaction and thoughts going on in their minds. Since users have different interactions and experiences, it is very important to understand each group and make proper accommodations to the design.</li>\n                    <li>Although this project focuses on understanding the user journey, I already identified several affordance problems with the interface that I failed to notice when I personally interacted with the interface for the first time - just another example of how important it is to step into different users' shoes to improve a design.</li>\n                    <li>For the future projects, trying to understand the users should always be the first step.</li>\n                    <li>(Standing right next to the machine and staring at people when they were waiting for their coffee while pretending that I was not staring was fun and challenging XD)</li>\n                </ul>\n                <br/><br/><br/><br/>\n         </div>\n        </div>\n\n        <div className='footer'> Juliana 2022<br/>Hand-coded with &#9829;</div>\n\n</div>\n  );\n}\n\nexport default Persona;\n","/Users/julianahyc/Desktop/Senior/SeniorüçÅ/portfolio/src/pages/Development.js",["115"],[],"import React from 'react';\nimport './Project.css';\nimport { useEffect } from 'react';\n\nfunction Development() {\n  useEffect(() => {\n    window.scrollTo(0, 0)\n  }, [])\n  return (\n    <div className=\"Project\">\n        <div className='back_button'>\n           <a href='/CS1300_Portfolio'><button> &lt;  Back to Projects</button><br/><br/></a> \n        </div>\n        <div className='Project_intro'>\n          <p style={{fontSize:'4rem',display:'inline'}}> JHan's Virtual Gallery <br/><br/> </p>\n          <p style={{color:'#4682B4',display:'inline'}}><b>Implementing </b></p> an interactive gallery webpage using components and updated data state while incorporating visual design principles.\n        </div>\n\n        <div className='Project_content'>\n        <div className='Project_context'>\n            <div className='section_title'>Context & Aim</div>\n                <p>In this project, I implemented a gallery webpage using <a href={\"https://reactjs.org/\"} id=\"react\">React</a>, a JavaScript library for an interactive user interface. In this virtual gallery, I have selected 12 of my personal favorite paintings and displayed information including the artist, year of production, media, current location, and dimension, along with a sentence about what I like about the art piece. The virtual gallery allows the user to sort, filter, and aggregate the dimensions of selected artworks.</p>\n                <p>Through this project, I aim to gain familiarity with developing functional websites using a common JS framework to accompany all the design concepts and methods that I have learned in previous projects, while still incorporating necessary visual design principles. I hope to improve <b>user experience</b> through both practical and aesthetic elements.</p>\n         </div>\n\n         <div className='Project_process'>\n            <div className='section_title'>Process</div>\n\n            <div className='section_subtitle'>STEP 1: Getting familiar with React & JS</div>\n            <p>I began with learning some basic concepts, functions, and syntax of JavaScript and React. These include: </p>\n            <ul>\n                <li>Components</li>\n                <li>Props</li>\n                <li>State and useState()</li>\n                <li>filter(), sort(),map(),etc.</li>\n            </ul>\n            <br/>\n            \n\n            <div className='section_subtitle'>STEP 2: Deciding on visual design choices</div>\n                <p>I chose to use a simple layout for the webpage, with panels of filters and sorting buttons on the left and the list of artworks on the right. The webpage has a unified color scheme with white, gray, and blue for a clean and concise look. I used the Material UI library for the action buttons to have a consistent theme. Each artwork takes up a card with different font sizes, colors, and italicizing to create a visual hierarchy.</p>\n            <br/>   \n\n    {/* <div className='section_subtitle'>STEP 3: Implementing the webpage</div>\n      <p>Out of the 5 users I observed, I conducted interviews with 3 users.</p> */}\n\n\n\n\n         </div>\n\n         <div className='Project_deliverables'>\n            <div className='section_title'>deliverables</div>\n            \n            Check out my gallery: <a className=\"responsive_url\" href={\"https://sprintingsloth420.github.io/CS1300_Development/\"}>JHan's virtual gallery</a>\n            <br/> <br/>\n            <iframe className='iframe' src={\"https://sprintingsloth420.github.io/CS1300_Development/\"}></iframe>\n            <br/><br/>\n\n           \n        </div>\n\n         <div className='Project_takeaways'>\n            <div className='section_title'>Takeaways</div>\n            <ul style={{lineHeight:'2.8rem'}}>\n                    <li>This is my first fully functional interactive webpage! </li>\n                   <li>I gained experience with webpage development tools, including JavaScript, React, and the Material UI library. The concepts of props and state are particularly useful and should be applicable to different projects in the future.</li>\n                   <li>(Selecting artworks to display took me a while, but I enjoy looking at something aesthetically pleasing and functional built by myself :P )</li>\n                </ul>\n                <br/><br/><br/><br/>\n         </div>\n        </div>\n\n        <div className='footer'> Juliana 2022<br/>Handcoded with &#9829;</div>\n\n</div>\n  );\n}\n\nexport default Development;\n","/Users/julianahyc/Desktop/Senior/SeniorüçÅ/portfolio/src/pages/About.js",[],[],"/Users/julianahyc/Desktop/Senior/SeniorüçÅ/portfolio/src/pages/Home.js",[],[],"/Users/julianahyc/Desktop/Senior/SeniorüçÅ/portfolio/src/pages/Art.js",["116","117"],[],"import React from 'react';\nimport './HomeNav.css';\n\n\nfunction Art() {\n  return (\n    <div className=\"Art\">\n      <div className=\"ArtText\">I DO SOME ART :)<br/><br/> COMING SOON...</div>\n      <div className=\"ArtImage\"><img src={process.env.PUBLIC_URL + \"/\" + \"images/painting.jpg\"}></img></div>\n       \n\n    </div>\n  );\n}\n\nexport default Art;\n","/Users/julianahyc/Desktop/Senior/SeniorüçÅ/portfolio/src/pages/ProjectItem.js",["118"],[],"import * as React from 'react';\nimport \"./HomeNav.css\";\n// import Card from '@mui/material/Card';\n// import CardActions from '@mui/material/CardActions';\n// import CardContent from '@mui/material/CardContent';\n// import CardMedia from '@mui/material/CardMedia';\n// import Button from '@mui/material/Button';\n// import Typography from '@mui/material/Typography';\n\n\nexport default function ProjectItem({item}) {\n  return (\n    <div className='projectItem'> <a href={item.url}>\n      <div><img src={item.image}></img></div>\n      <div>\n        <p className='projectItem_title'>{item.name}</p>\n        <div className='tag_wrapper'>\n          <div className='tag'>{item.tag}</div>\n          <div className='tag'>{item.tag2}</div>\n        </div>\n        <div className='projectItem_des'>{item.description}</div>\n      </div>\n\n      {/* <Card>\n\n    <CardContent> */}\n      {/* <Button variant=\"outlined\" >{item.tag}</Button>\n      <Button variant=\"outlined\" >{item.tag2}</Button>\n      <p></p> */}\n      {/* <Typography variant=\"body1\" color=\"text.secondary\">\n        {item.description}\n      </Typography>\n      <p></p> */}\n      {/* <a target=\"_blank\" href={item.url}>View Project Details</a> */}\n    {/* </CardContent>\n    <CardActions> */}\n      {/* <Button size=\"small\">Learn More</Button> */}\n      \n    {/* </CardActions>\n  </Card> */}\n    </a>\n    </div>\n     \n\n    \n  );\n}",{"ruleId":"119","severity":1,"message":"120","line":2,"column":36,"nodeType":"121","messageId":"122","endLine":2,"endColumn":40},{"ruleId":"119","severity":1,"message":"123","line":13,"column":8,"nodeType":"121","messageId":"122","endLine":13,"endColumn":13},{"ruleId":"124","severity":1,"message":"125","line":34,"column":15,"nodeType":"126","endLine":34,"endColumn":111},{"ruleId":"127","severity":1,"message":"128","line":34,"column":76,"nodeType":"129","messageId":"130","endLine":34,"endColumn":77},{"ruleId":"124","severity":1,"message":"125","line":47,"column":17,"nodeType":"126","endLine":47,"endColumn":114},{"ruleId":"127","severity":1,"message":"128","line":47,"column":78,"nodeType":"129","messageId":"130","endLine":47,"endColumn":79},{"ruleId":"124","severity":1,"message":"125","line":52,"column":5,"nodeType":"126","endLine":52,"endColumn":241},{"ruleId":"131","severity":1,"message":"132","line":53,"column":5,"nodeType":"126","messageId":"133","endLine":53,"endColumn":294,"fix":"134"},{"ruleId":"124","severity":1,"message":"125","line":390,"column":17,"nodeType":"126","endLine":390,"endColumn":253},{"ruleId":"131","severity":1,"