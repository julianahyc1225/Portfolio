"endColumn":103},{"ruleId":"127","severity":1,"message":"128","line":106,"column":76,"nodeType":"129","messageId":"130","endLine":106,"endColumn":77},{"ruleId":"124","severity":1,"message":"125","line":112,"column":13,"nodeType":"126","endLine":112,"endColumn":101},{"ruleId":"136","severity":1,"message":"137","line":34,"column":13,"nodeType":"126","endLine":34,"endColumn":83},{"ruleId":"127","severity":1,"message":"128","line":34,"column":52,"nodeType":"129","messageId":"130","endLine":34,"endColumn":53},{"ruleId":"136","severity":1,"message":"137","line":120,"column":17,"nodeType":"126","endLine":120,"endColumn":81},{"ruleId":"127","severity":1,"message":"128","line":120,"column":56,"nodeType":"129","messageId":"130","endLine":120,"endColumn":57},{"ruleId":"136","severity":1,"message":"137","line":141,"column":18,"nodeType":"126","endLine":141,"endColumn":82},{"ruleId":"127","severity":1,"message":"128","line":141,"column":57,"nodeType":"129","messageId":"130","endLine":141,"endColumn":58},{"ruleId":"136","severity":1,"message":"137","line":147,"column":13,"nodeType":"126","endLine":147,"endColumn":121},{"ruleId":"127","severity":1,"message":"128","line":147,"column":76,"nodeType":"129","messageId":"130","endLine":147,"endColumn":77},{"ruleId":"124","severity":1,"message":"125","line":57,"column":13,"nodeType":"126","endLine":57,"endColumn":104},{"ruleId":"136","severity":1,"message":"137","line":9,"column":33,"nodeType":"126","endLine":9,"endColumn":97},{"ruleId":"127","severity":1,"message":"128","line":9,"column":72,"nodeType":"129","messageId":"130","endLine":9,"endColumn":73},{"ruleId":"136","severity":1,"message":"137","line":14,"column":12,"nodeType":"126","endLine":14,"endColumn":34},"no-unused-vars","'Link' is defined but never used.","Identifier","unusedVar","'About' is defined but never used.","jsx-a11y/iframe-has-title","<iframe> elements must have a unique title property.","JSXOpeningElement","no-useless-concat","Unexpected string concatenation of literals.","BinaryExpression","unexpectedConcat","react/jsx-no-target-blank","Using target=\"_blank\" without rel=\"noreferrer\" (which implies rel=\"noopener\") is a security risk in older browsers: see https://mathiasbynens.github.io/rel-noopener/#recommendations","noTargetBlankWithoutNoreferrer",{"range":"141","text":"142"},{"range":"143","text":"142"},"jsx-a11y/alt-text","img elements must have an alt prop, either with meaningful text, or an empty string for decorative images.",{"range":"144","text":"142"},{"range":"145","text":"142"},{"range":"146","text":"142"},[5123,5123]," rel=\"noreferrer\"",[18766,18766],[33615,33615],[33994,33994],[34391,34391]]